<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			
			<tr>
				<td>ID</td>
				<td>Título</td>
				<td>Texto</td>
				<td>Autor</td>
				<td>Data</td>
			</tr>
			<% for(var i = 0; i < posts.length; i++){ %>
				<tr>
					<td><%= posts[i].id_post %></td>
					<td><%= posts[i].titulo %></td>
					<td><%= posts[i].post %></td>
					<td><%= posts[i].autor %></td>
					<td><%= posts[i].data_criacao %></td>
				</tr>
			<% } %>

		</table>

	</body>
</html>